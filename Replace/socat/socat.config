# forward port 8000 on IPv6 to IPv4 host port 80
# change enable to '1' to use this example
config socat 'http'
	option enable '0'
	option SocatOptions '-d -d TCP6-LISTEN:8000,fork TCP4:192.168.1.20:80'
	option user 'nobody'

config config 'router'
	option firewall_accept '1'
	option enable '1'
	option remarks 'router'
	option protocol 'port_forwards'
	option family '6'
	option proto 'tcp'
	option listen_port '8000'
	option reuseaddr '1'
	option dest_proto 'tcp4'
	option dest_ip '192.168.2.1'
	option dest_port '80'

